{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "pluggins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "camelcase": [
            "error"
        ],
        "eqeqeq": [
            "error",
            "always"
        ]
    }
}